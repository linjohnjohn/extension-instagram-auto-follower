{
  "name": "IG Auto Follower",
  "version": "2.0",
  "description": "Automate your instagram Follow for Follow strategy!",
  "permissions": ["debugger", "storage"],
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": ["/static/js/background.js", "/static/js/0.chunk.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "css": ["/static/css/1.chunk.css", "/static/css/content.css"],
      "js": ["/static/js/1.chunk.js", "/static/js/content.js"]
    }
  ],
  "web_accessible_resources": ["/help/help.html"],
  "content_security_policy": "script-src 'self' 'sha256-FRoFlGx2+Fhv0tPS9pYdsPweLbLLjRfa4Ap2BYiXk2E='; object-src 'self'"
}
